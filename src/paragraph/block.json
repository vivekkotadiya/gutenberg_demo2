{
	"$schema": "https://json.schemastore.org/block.json",
	"apiVersion": 2,
	"name": "inxperts-gutenberg-blocks/paragraph",
	"version": "0.1.5",
	"title": "Paragraph",
	"category": "theme",
	"keywords": [ "paragraph", "detail" ],
	"description": "A gutenberg block to use for Paragraph.",
	"supports": {
		"html": false,
		"anchor": true
	},
	"attributes": {
		"textAlign": {
			"type": "string",
			"default": "text-left"
		},
		"content": {
			"type": "string",
			"source": "html",
			"selector": "p,span",
			"default": ""
		},
		"tag": {
			"type": "string",
			"default": "p"
		},
		"placeholder": {
			"type": "string",
			"default": "Add text here..."
		},
		"style": {
			"type": "string",
			"default": "text-grey"
		}
	},
	"textdomain": "inxperts-gutenberg-blocks"
}
