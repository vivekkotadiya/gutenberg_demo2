{
	"$schema": "https://json.schemastore.org/block.json",
	"apiVersion": 2,
	"name": "inxperts-gutenberg-blocks/inxperts-image",
	"version": "0.1.5",
	"title": "Image",
	"category": "theme",
    "keywords": ["image", "picture", "inxperts"],
	"description": "A gutenberg block to use for Image.",
