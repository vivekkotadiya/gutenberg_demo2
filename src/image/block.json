	"attributes": {
        "imageUrl" : {
            "type": "string",
            "default": ""
        },
		"xlimageUrl" : {
			"type": "string",
			"default": ""
		},
        "lgimageUrl" : {
			"type": "string",
			"default": ""
		},
        "mdimageUrl" : {
			"type": "string",
			"default": ""
		},
        "smimageUrl" : {
			"type": "string",
			"default": ""
		},
        "imageId" : {
            "type": "number",
            "default": ""
        },
        "altText" : {
            "type": "string",
            "default": ""
        }
	},
	"textdomain": "inxperts-gutenberg-blocks"
}
